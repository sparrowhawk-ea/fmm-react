	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"name": "@eafmm/react",
	"peerDependencies": {
		"@eafmm/core": "3"
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"printWidth": 132,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sparrowhawk-ea/fmm-react.git"
	},
	"scripts": {
		"build": "rm -rf demo dist && npm run lint && rollup -c && npm run gzip",
		"gzip": "gzip -9 -c dist/index.js > dist/index.js.gz && gzip -9 -c dist/index.mjs > dist/index.mjs.gz && gzip -9 -c demo/index.js > demo/index.js.gz",
		"lint": "eslint --report-unused-disable-directives --ext .ts,.tsx .",
		"prepare": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"typings": "dist/index.d.ts",
	"version": "3.0.1"
}
